{
  "name": "zoha/laravel-meta",
  "description": "a package for working with models meta",
  "keywords": [
    "meta",
    "laravel",
    "data",
    "package"
  ],
  "type": "package",
  "license": "MIT",
  "source": {
    "url": "https://github.com/ZohaBanam/laravel-meta.git",
    "type": "git",
    "reference": "0ac3f7b"
  },
  "require": {
    "laravel/framework": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "php": ">=7.4",
    "phpunit/phpunit": "^9.5|^10.0|^11.0",
    "orchestra/testbench": "^8.0"
  },
  "authors": [
    {
      "name": "Zoha Banam",
      "email": "zoha.banam@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zoha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zoha\\Meta\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zoha\\Meta\\MetaServiceProvider"
      ],
      "aliases": {
        "Meta": "Zoha\\Meta\\Facades\\MetaFacade"
      }
    }
  }
}
